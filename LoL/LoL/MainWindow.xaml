<Window x:Class="LoL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="League Scanner" Height="768" Width="1024" WindowStartupLocation="CenterScreen"
        MinWidth="800"
        MinHeight="600">
    <Grid Background="#8AD3F3">
        <TabControl x:Name="tcMain" HorizontalAlignment="Stretch" Margin="0,97,0,10" VerticalAlignment="Stretch" >
            <TabItem Header="Home" FontSize="16" Padding="10,0,10,0" >
                <TabItem.Style>
                    <Style TargetType="{x:Type TabItem}">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="{x:Type TabItem}">
                                    <Grid>
                                        <Border  Name="Border" Margin="0 0 0 0" BorderThickness="0 0 0 0" >
                                            <ContentPresenter x:Name="ContentSite" VerticalAlignment="Center" HorizontalAlignment="Center" ContentSource="Header" Margin="12,2,12,2" RecognizesAccessKey="True"/>
                                        </Border>
                                    </Grid>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter TargetName="Border" Property="Background" Value="Blue" /> 
                                            <Setter Property="Foreground" Value="White" />
                                            <Setter TargetName="Border" Property="BorderThickness" Value="0 0 0 0" /> 
                                        </Trigger> 
                                        <Trigger Property="IsSelected" Value="False">
                                            <Setter Property="Foreground" Value="White"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </TabItem.Style>
                <Grid x:Name="gridMain" Background="Gray" >
                    <!-- <Grid.RowDefinitions>
                        <RowDefinition Height="120"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="40*"/>
                        <ColumnDefinition Width="10*"/>
                        <ColumnDefinition Width="50*"/>
                    </Grid.ColumnDefinitions>
                    <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10 10 0 0" Visibility="{Binding SummonerInfoVisibility}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="50*"/>
                                <RowDefinition Height="50*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Row="0" Grid.Column="0" FontSize="22" Margin="0 0 10 0">Summoner:</TextBlock>
                            <TextBlock Grid.Row="0" Grid.Column="1" FontSize="22" Foreground="Red" Text="{Binding SummonerTitle}"></TextBlock>
                            <TextBlock Grid.Row="1" Grid.Column="0" FontSize="18" Margin="0 0 10 0" >Level:</TextBlock>
                            <TextBlock Grid.Row="1" Grid.Column="1" FontSize="18" Foreground="Blue" Text="{Binding SummonerLevel}"></TextBlock>
                        </Grid>
                    </StackPanel> -->
                    <TextBlock FontSize="60" Margin="0 100 0 0" VerticalAlignment="Top" HorizontalAlignment="Center" Foreground="White" FontFamily="DengXian">LOL Scanner</TextBlock>
                    <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center">
                        <StackPanel Orientation="Horizontal">
                            <ComboBox ItemsSource="{Binding Regions}"  DisplayMemberPath="Name"  SelectedValuePath="Code" SelectedItem="{Binding Path=SelectedRegion, Mode=TwoWay}" Margin="0 0 10 0" Height="30" Width="170" VerticalAlignment="Center" VerticalContentAlignment="Center"  Background="#8AD3F3">
                            </ComboBox>
                            <TextBox Background="#8AD3F3" x:Name="tbSummonerName"  FontSize="16" Text="{Binding Path=SummonerName, Mode=TwoWay}" GotMouseCapture="tbSummonerName_GotMouseCapture" Margin="0 0 10 0" Height="30" Width="200" VerticalAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            <Button x:Name="btnSearch"  FontSize="16" Click="Button_Click" KeyDown="Button_KeyDown" Foreground="White" Height="30" Width="60" VerticalAlignment="Center" Content="Search">
                                <Button.Style>
                                    <Style TargetType="{x:Type Button}">
                                        <Setter Property="Background" Value="Blue"/>
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="{x:Type Button}">
                                                    <Border Background="{TemplateBinding Background}">
                                                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                    </Border>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                        <Style.Triggers>
                                            <Trigger Property="IsMouseOver" Value="True">
                                                <Setter Property="Background" Value="Blue"/>
                                            </Trigger>
                                            <Trigger Property="IsPressed" Value="True">
                                                <Setter Property="Background" Value="DarkBlue"/>
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                        </StackPanel>
                    </StackPanel>
                </Grid>
            </TabItem>
            <TabItem Header="Summoner Lookup" FontSize="16"  Padding="10,0,10,0" IsEnabled="{Binding TabPagesEnabled}">
                <TabItem.Style>
                    <Style TargetType="{x:Type TabItem}">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="{x:Type TabItem}">
                                    <Grid>
                                        <Border  Name="Border" Margin="0 0 0 0" BorderThickness="0 0 0 0" >
                                            <ContentPresenter x:Name="ContentSite" VerticalAlignment="Center" HorizontalAlignment="Center" ContentSource="Header" Margin="12,2,12,2" RecognizesAccessKey="True"/>
                                        </Border>
                                    </Grid>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter TargetName="Border" Property="Background" Value="Blue" />
                                            <Setter Property="Foreground" Value="White" />
                                            <Setter TargetName="Border" Property="BorderThickness" Value="0 0 0 0" />
                                        </Trigger>
                                        <Trigger Property="IsSelected" Value="False">
                                            <Setter Property="Foreground" Value="White"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </TabItem.Style>
                <Grid Background="Black" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="9*"/>
                        <ColumnDefinition Width="496*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="100"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="100"/>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="10 0 0 0">Top five champions</TextBlock>
                    <Grid Grid.Row="1" Grid.ColumnSpan="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>

                        </Grid.ColumnDefinitions>
                        <Border Grid.Row="0" Grid.Column="0" BorderBrush="White" BorderThickness="0 0.3 0.3 0.3" SnapsToDevicePixels="True"/>
                        <Image Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center"  Margin="1 1 1 1"  Source="{Binding Champ1Image}" Opacity="0.7"/>
                        <StackPanel Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" >
                            <TextBlock FontSize="18" HorizontalAlignment="Center" Text="{Binding Champ1Name}"></TextBlock>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Row="0" Grid.Column="0" FontSize="14" Margin="0 0 10 0" >Games:</TextBlock>
                                <TextBlock Grid.Row="0" Grid.Column="1" FontSize="14" Text="{Binding Champ1Games}" Foreground="LightBlue"/>
                                <TextBlock Grid.Row="1" Grid.Column="0" FontSize="14" Margin="0 0 10 0" >Wins:</TextBlock>
                                <TextBlock Grid.Row="1" Grid.Column="1" FontSize="14" Text="{Binding Champ1Wins}" Foreground="Green"/>
                                <TextBlock Grid.Row="2" Grid.Column="0"  FontSize="14" Margin="0 0 10 0" >Losses:</TextBlock>
                                <TextBlock Grid.Row="2" Grid.Column="1"  FontSize="14" Text="{Binding Champ1Looses}" Foreground="Red"/>
                            </Grid>
                        </StackPanel> 
                        <Border Grid.Row="0" Grid.Column="1" BorderBrush="White" BorderThickness="0 0.3 0.3 0.3" SnapsToDevicePixels="True"/>
                        <Image Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="1 1 1 1"  Source="{Binding Champ2Image}" Opacity="0.7"/>
                        <StackPanel Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" >
                            <TextBlock FontSize="18" HorizontalAlignment="Center" Text="{Binding Champ2Name}"></TextBlock>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Row="0" Grid.Column="0" FontSize="14" Margin="0 0 10 0" >Games:</TextBlock>
                                <TextBlock Grid.Row="0" Grid.Column="1" FontSize="14" Text="{Binding Champ2Games}" Foreground="LightBlue"/>
                                <TextBlock Grid.Row="1" Grid.Column="0" FontSize="14" Margin="0 0 10 0" >Wins:</TextBlock>
                                <TextBlock Grid.Row="1" Grid.Column="1" FontSize="14" Text="{Binding Champ2Wins}" Foreground="Green"/>
                                <TextBlock Grid.Row="2" Grid.Column="0"  FontSize="14" Margin="0 0 10 0" >Losses:</TextBlock>
                                <TextBlock Grid.Row="2" Grid.Column="1"  FontSize="14" Text="{Binding Champ2Looses}" Foreground="Red"/>
                            </Grid>
                        </StackPanel>
                        <Border Grid.Row="0" Grid.Column="2" BorderBrush="White" BorderThickness="0 0.3 0.3 0.3" SnapsToDevicePixels="True"/>
                        <Image Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center"  Margin="1 1 1 1" Source="{Binding Champ3Image}" Opacity="0.7"/>
                        <StackPanel Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" >
                            <TextBlock FontSize="18" HorizontalAlignment="Center" Text="{Binding Champ3Name}"></TextBlock>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Row="0" Grid.Column="0" FontSize="14" Margin="0 0 10 0" >Games:</TextBlock>
                                <TextBlock Grid.Row="0" Grid.Column="1" FontSize="14" Text="{Binding Champ3Games}" Foreground="LightBlue"/>
                                <TextBlock Grid.Row="1" Grid.Column="0" FontSize="14" Margin="0 0 10 0" >Wins:</TextBlock>
                                <TextBlock Grid.Row="1" Grid.Column="1" FontSize="14" Text="{Binding Champ3Wins}" Foreground="Green"/>
                                <TextBlock Grid.Row="2" Grid.Column="0"  FontSize="14" Margin="0 0 10 0" >Losses:</TextBlock>
                                <TextBlock Grid.Row="2" Grid.Column="1"  FontSize="14" Text="{Binding Champ3Looses}" Foreground="Red"/>
                            </Grid>
                        </StackPanel>
                        <Border Grid.Row="0" Grid.Column="3" BorderBrush="White" BorderThickness="0 0.3 0.3 0.3" SnapsToDevicePixels="True"/>
                        <Image Grid.Row="0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="1 1 1 1" Source="{Binding Champ4Image}" Opacity="0.7"/>
                        <StackPanel Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" >
                            <TextBlock FontSize="18" HorizontalAlignment="Center" Text="{Binding Champ4Name}"></TextBlock>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Row="0" Grid.Column="0" FontSize="14" Margin="0 0 10 0" >Games:</TextBlock>
                                <TextBlock Grid.Row="0" Grid.Column="1" FontSize="14" Text="{Binding Champ4Games}" Foreground="LightBlue"/>
                                <TextBlock Grid.Row="1" Grid.Column="0" FontSize="14" Margin="0 0 10 0" >Wins:</TextBlock>
                                <TextBlock Grid.Row="1" Grid.Column="1" FontSize="14" Text="{Binding Champ4Wins}" Foreground="Green"/>
                                <TextBlock Grid.Row="2" Grid.Column="0"  FontSize="14" Margin="0 0 10 0" >Losses:</TextBlock>
                                <TextBlock Grid.Row="2" Grid.Column="1"  FontSize="14" Text="{Binding Champ4Looses}" Foreground="Red"/>
                            </Grid>
                        </StackPanel>
                        <Border Grid.Row="0" Grid.Column="4" BorderBrush="White" BorderThickness="0 0.3 0 0.3" SnapsToDevicePixels="True"/>
                        <Image Grid.Row="0" Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="1 1 1 1" Source="{Binding Champ5Image}" Opacity="0.7"/>
                        <StackPanel Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" >
                            <TextBlock FontSize="18" HorizontalAlignment="Center" Text="{Binding Champ5Name}"></TextBlock>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Row="0" Grid.Column="0" FontSize="14" Margin="0 0 10 0">Games:</TextBlock>
                                <TextBlock Grid.Row="0" Grid.Column="1" FontSize="14" Text="{Binding Champ5Games}" Foreground="LightBlue"/>
                                <TextBlock Grid.Row="1" Grid.Column="0" FontSize="14" Margin="0 0 10 0" >Wins:</TextBlock>
                                <TextBlock Grid.Row="1" Grid.Column="1" FontSize="14" Text="{Binding Champ5Wins}" Foreground="Green"/>
                                <TextBlock Grid.Row="2" Grid.Column="0"  FontSize="14" Margin="0 0 10 0" >Losses:</TextBlock>
                                <TextBlock Grid.Row="2" Grid.Column="1"  FontSize="14" Text="{Binding Champ5Looses}" Foreground="Red"/>
                            </Grid>
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Row="2" Grid.ColumnSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="15*"/>
                            <ColumnDefinition Width="15*"/>
                            <ColumnDefinition Width="15*"/>
                            <ColumnDefinition Width="50*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Margin="10 0 0 0" VerticalAlignment="Center" Grid.Row="0">Ranked Ratings</TextBlock>
                        <Border Grid.Row="1" Grid.Column="0" BorderBrush="White" BorderThickness="0 0.3 0.3 0.3" SnapsToDevicePixels="True"/>
                        <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Top" >
                            <TextBlock Margin="0 10 0 0" FontSize="18" HorizontalAlignment="Center">Team 3v3</TextBlock>
                            <Image  Margin="1 1 1 20" Source="{Binding Team3v3Image}"/>
                            <!--<TextBlock  FontSize="18" HorizontalAlignment="Center">Stats</TextBlock> -->
                        </StackPanel>
                        <Border Grid.Row="1" Grid.Column="1" BorderBrush="White" BorderThickness="0 0.3 0.3 0.3" SnapsToDevicePixels="True"/>
                        <StackPanel Grid.Row="1"  Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Top" >
                            <TextBlock  Margin="0 10 0 0"  FontSize="18" HorizontalAlignment="Center">Solo 5v5</TextBlock>
                            <Image Margin="1 1 1 20" Source="{Binding Solo5v5Image}"/>
                            <!--<TextBlock  FontSize="18" HorizontalAlignment="Center">Stats</TextBlock> -->
                        </StackPanel>
                        <Border Grid.Row="1" Grid.Column="2" BorderBrush="White" BorderThickness="0 0.3 0 0.3" SnapsToDevicePixels="True"/>
                        <StackPanel Grid.Row="1"  Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Top" >
                            <TextBlock Margin="0 10 0 0"  FontSize="18" HorizontalAlignment="Center">Team 5v5</TextBlock>
                            <Image  Margin="1 1 1 20" Source="{Binding Team5v5Image}"/>
                            <!--<TextBlock  FontSize="18" HorizontalAlignment="Center">Stats</TextBlock> -->
                        </StackPanel>
                        <Border Grid.Row="0" Grid.Column="3" BorderBrush="White" BorderThickness="0.3 0 0 0" SnapsToDevicePixels="True"/>
                        <Border Grid.Row="1" Grid.Column="3" BorderBrush="White" BorderThickness="0.3 0.3 0 0.3" SnapsToDevicePixels="True"/>
                        <TextBlock Margin="10 0 0 0"   Grid.Column="4" VerticalAlignment="Center">Statistics</TextBlock>
                        <Grid Grid.Row="1" Grid.Column="3" >
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <StackPanel  Grid.Row="0" Grid.Column="0" Orientation="Vertical" VerticalAlignment="Top" >
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="55*"/>
                                            <ColumnDefinition Width="40*"/>
                                            <ColumnDefinition Width="55*"/>
                                            <ColumnDefinition Width="40*"/>
                                            <ColumnDefinition Width="70*"/>
                                            <ColumnDefinition Width="40*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Grid.Row="0" Grid.Column="0" FontSize="16"  Grid.ColumnSpan="2"  VerticalAlignment="Center" HorizontalAlignment="Center" >Ranked 5v5:</TextBlock>
                                        <TextBlock Grid.Row="1" Grid.Column="0" FontSize="14" Margin="5 0 10 0" >Kills:</TextBlock>
                                        <TextBlock Grid.Row="1" Grid.Column="1" FontSize="14" Text="{Binding KillsRanked}" Foreground="Green"/>
                                        <TextBlock Grid.Row="2" Grid.Column="0" FontSize="14" Margin="5 0 10 0" >Deaths:</TextBlock>
                                        <TextBlock Grid.Row="2" Grid.Column="1" FontSize="14" Text="{Binding DeathsRanked}" Foreground="Green"/>
                                        <TextBlock Grid.Row="3" Grid.Column="0"  FontSize="14" Margin="5 0 10 0" >Assists:</TextBlock>
                                        <TextBlock Grid.Row="3" Grid.Column="1"  FontSize="14" Text="{Binding AssistsRanked}" Foreground="Green"/>
                                        <TextBlock Grid.Row="4" Grid.Column="0"  FontSize="14" Margin="5 0 10 0" >Double kills:</TextBlock>
                                        <TextBlock Grid.Row="4" Grid.Column="1"  FontSize="14" Text="{Binding DoubleKills}" Foreground="Green"/>
                                        <TextBlock Grid.Row="5" Grid.Column="0"  FontSize="14" Margin="5 0 10 0" >Triple kills:</TextBlock>
                                        <TextBlock Grid.Row="5" Grid.Column="1"  FontSize="14" Text="{Binding TripleKills}" Foreground="Green"/>
                                        <TextBlock Grid.Row="6" Grid.Column="0"  FontSize="14" Margin="5 0 10 0" >Quadra kills:</TextBlock>
                                        <TextBlock Grid.Row="6" Grid.Column="1"  FontSize="14" Text="{Binding QuadraKills}" Foreground="Green"/>
                                        <TextBlock Grid.Row="7" Grid.Column="0"  FontSize="14" Margin="5 0 10 0" >Penta kills:</TextBlock>
                                        <TextBlock Grid.Row="7" Grid.Column="1"  FontSize="14" Text="{Binding PentaKills}" Foreground="Green"/>
                                        <TextBlock Grid.Row="8" Grid.Column="0"  FontSize="14" Margin="5 0 10 5" >W/L:</TextBlock>
                                        <TextBlock Grid.Row="8" Grid.Column="1"  FontSize="14" Text="{Binding WLRanked}" Foreground="Green"/>


                                        <TextBlock Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="2" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Center" >Normal 5v5:</TextBlock>
                                        <TextBlock Grid.Row="1" Grid.Column="2"  FontSize="14" Margin="5 0 10 0" >Kills:</TextBlock>
                                        <TextBlock Grid.Row="1" Grid.Column="3"  FontSize="14" Text="{Binding KillsNormal}" Foreground="Green"/>
                                       <!-- <TextBlock Grid.Row="2" Grid.Column="2"  FontSize="14" Margin="5 0 10 0">Deaths:</TextBlock>
                                        <TextBlock Grid.Row="2" Grid.Column="3"  FontSize="14" Text="{Binding DeathsNormal}" Foreground="Green"/>
                                        <TextBlock Grid.Row="3" Grid.Column="2"  FontSize="14" Margin="5 0 10 0">Assists:</TextBlock>
                                        <TextBlock Grid.Row="3" Grid.Column="3"  FontSize="14" Text="{Binding AssistsNormal}" Foreground="Green"/>
                                        <TextBlock Grid.Row="4" Grid.Column="2"  FontSize="14" Margin="5 0 10 0">Wins:</TextBlock>
                                        <TextBlock Grid.Row="4" Grid.Column="3"  FontSize="14" Text="{Binding WinsNormal}" Foreground="Green"/> -->
                                        <TextBlock Grid.Row="2" Grid.Column="2"  FontSize="14" Margin="5 0 10 0" >Assists:</TextBlock>
                                        <TextBlock Grid.Row="2" Grid.Column="3"  FontSize="14" Text="{Binding AssistsNormal}" Foreground="Green"/>
                                        <TextBlock Grid.Row="3" Grid.Column="2"  FontSize="14" Margin="5 0 10 0" >Wins:</TextBlock>
                                        <TextBlock Grid.Row="3" Grid.Column="3"  FontSize="14" Text="{Binding WinsNormal}" Foreground="Green"/>

                                        <TextBlock Grid.Row="10" Grid.Column="0" Grid.ColumnSpan="6" FontSize="16" Margin="10 5 10 5" >Ranked Averages:</TextBlock>
                                        <TextBlock Grid.Row="11" Grid.Column="0" FontSize="14" Margin="5 0 10 0" >Average KDA:</TextBlock>
                                        <TextBlock Grid.Row="11" Grid.Column="1" FontSize="14" Text="{Binding AverageKDA}" Foreground="Green"/>
                                        <!-- <TextBlock Grid.Row="1" Grid.Column="3" FontSize="14" Text="{Binding AverageKills}" Foreground="Green"/> -->
                                        <TextBlock Grid.Row="12" Grid.Column="0" FontSize="14" Margin="5 0 0 0" >Average Gold:</TextBlock>
                                        <TextBlock Grid.Row="12" Grid.Column="1" FontSize="14" Text="{Binding AverageGold}" Foreground="Green"/>
