                                        <TextBlock Grid.Row="13" Grid.Column="1"  FontSize="14" Text="{Binding AverageFarm}" Foreground="Green"/>
                                        <!--  <TextBlock Grid.Row="3" Grid.Column="3"  FontSize="14" Text="{Binding AverageGold}" Foreground="Green"/> 
                                        <TextBlock Grid.Row="4" Grid.Column="2"  FontSize="14" Margin="0 0 10 0">Farms:</TextBlock>
                                        <TextBlock Grid.Row="4" Grid.Column="3"  FontSize="14" Text="{Binding AverageFarm}" Foreground="Green"/>-->
                                        <TextBlock Grid.Row="11" Grid.Column="2" Grid.ColumnSpan="2" FontSize="14" Margin="0 0 50 0" >Total time played:</TextBlock>

                                        <StackPanel Grid.Row="11" Grid.Column="3" Margin="20,0,0,0" Grid.ColumnSpan="2" Orientation="Horizontal">
                                            <TextBlock  FontSize="14" Text="{Binding TotalHoursPlayed}"  Foreground="Green"/>
                                            <TextBlock  FontSize="14" Text="hours (" Margin="5,0,0,0"  />
                                            <TextBlock  FontSize="14" Text="{Binding TotalDaysPlayed}"  Foreground="Green"/>
                                            <TextBlock  FontSize="14" Text="days)" Margin="5,0,0,0"  />
                                        </StackPanel>

                                        <TextBlock Grid.Row="12" Grid.Column="2" Grid.ColumnSpan="2" FontSize="14" Margin="0,0,50,0" >Total:</TextBlock>
                                        <TextBlock Grid.Row="12" Grid.Column="3" Grid.ColumnSpan="2" FontSize="14" Text="{Binding TotalGames}" Margin="20,0,0,0" Foreground="Green"/>
                                        <!--<TextBlock Grid.Row="7" Grid.Column="2"  FontSize="14" Margin="0 0 10 0">Wins:</TextBlock>
                                        <TextBlock Grid.Row="8" Grid.Column="2"  FontSize="14" Margin="0 0 10 0">Losses:</TextBlock> -->
                                        <TextBlock Grid.Row="0" Grid.Column="4"  Grid.ColumnSpan="2" FontSize="16"  VerticalAlignment="Center" HorizontalAlignment="Center"  >Ward score:</TextBlock>
                                        <TextBlock Grid.Row="1" Grid.Column="4"  Grid.ColumnSpan="2" FontSize="14" Margin="5 0 60 0" >Your ward score is:</TextBlock>
                                        <TextBlock Grid.Row="1" Grid.Column="5"  FontSize="14" Text="{Binding Warding}" Margin="10 0 0 0" Foreground="Green"/>
                                        <TextBlock Grid.Row="2" Grid.Column="4"  FontSize="14" Margin="5 0 10 0" >Score:</TextBlock>
                                        <TextBlock Grid.Row="2" Grid.Column="5"  FontSize="14" Text="{Binding WardScore}" Foreground="Green"/>
                                        <TextBlock Grid.Row="3" Grid.Column="4"  FontSize="14" Margin="5 0 10 0" >Mage:</TextBlock>
                                        <TextBlock Grid.Row="3" Grid.Column="5"  FontSize="14" Text="{Binding WardMage}" Foreground="Green"/>
                                        <TextBlock Grid.Row="4" Grid.Column="4"  FontSize="14" Margin="5 0 10 0" >Assasin:</TextBlock>
                                        <TextBlock Grid.Row="4" Grid.Column="5"  FontSize="14" Text="{Binding WardAssasin}" Foreground="Green"/>
                                        <TextBlock Grid.Row="5" Grid.Column="4"  FontSize="14" Margin="5 0 10 0"  >Marskman:</TextBlock>
                                        <TextBlock Grid.Row="5" Grid.Column="5"  FontSize="14" Text="{Binding WardMarksman}" Foreground="Green"/>
