                            <TextBlock Grid.Column="4" FontSize="16" HorizontalAlignment="Center" Text="{Binding RChamp4Name}"/>
                            <StackPanel Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Bottom" >
                                <!-- <TextBlock FontSize="16" HorizontalAlignment="Center" Text="{Binding RChamp4Name}"/> -->
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0"  FontSize="12" Margin="0 0 10 0"><Run Text="K/D/A:"/></TextBlock>
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center"  Grid.Row="0" Grid.Column="1" Orientation="Horizontal">
                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Text="{Binding RChamp4Kills}"  Foreground="#22b14c" FontWeight="SemiBold"/>
                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="/" FontSize="12" />
                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Text="{Binding RChamp4Deaths}" Foreground="#ed1c24" FontWeight="SemiBold"/>
                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="/" FontSize="12"/>
                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Text="{Binding RChamp4Assists}" Foreground="#fffe00" FontWeight="SemiBold"/>
                                    </StackPanel>
                                    <TextBlock Grid.Row="1" Grid.Column="0"  FontSize="12" Margin="0 0 10 0" ><Run Text="W/L:"/></TextBlock>
                                    <StackPanel HorizontalAlignment="Left" VerticalAlignment="Center"  Grid.Row="1" Grid.Column="1" Orientation="Horizontal">
                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Text="{Binding RChamp4Wins}"  Foreground="#22b14c" FontWeight="SemiBold"/>
                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="/" FontSize="12" />
                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Text="{Binding RChamp4Losses}" Foreground="#ed1c24" FontWeight="SemiBold"/>
                                    </StackPanel>
                                </Grid>
                            </StackPanel>
                          
                        </Grid>
                    </Grid>

                </TabItem>


            </TabControl>
        </Grid>
    </Border>
</Window>
