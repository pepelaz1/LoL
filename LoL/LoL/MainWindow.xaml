                                            <TextBlock x:Name="tbSummoner" Text="Summoner" Padding="5" Margin="1" Background="#0f0f0f"></TextBlock>
                                        </StackPanel>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsSelected" Value="True">
